Â  Â  <script>
Â  Â  Â  Â  const postMoney = 6000000;
Â  Â  Â  Â  const fundSize = 70000000;
Â  Â  Â  Â Â 
Â  Â  Â  Â  function calculate() {
Â  Â  Â  Â  Â  Â  const invest = parseFloat(document.getElementById('investAmount').value);
Â  Â  Â  Â  Â  Â  const years = parseFloat(document.getElementById('exitYears').value);
Â  Â  Â  Â  Â  Â  const arr = parseFloat(document.getElementById('exitARR').value);
Â  Â  Â  Â  Â  Â  const multiple = parseFloat(document.getElementById('exitMultiple').value);
Â  Â  Â  Â  Â  Â  const dilution = parseFloat(document.getElementById('dilution').value) / 100;
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  document.getElementById('investDisplay').textContent = invest.toLocaleString();
Â  Â  Â  Â  Â  Â  document.getElementById('yearsDisplay').textContent = years;
Â  Â  Â  Â  Â  Â  document.getElementById('arrDisplay').textContent = arr;
Â  Â  Â  Â  Â  Â  document.getElementById('multipleDisplay').textContent = multiple;
Â  Â  Â  Â  Â  Â  document.getElementById('dilutionDisplay').textContent = (dilution * 100) + '%';
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  const ownership = invest / postMoney;
Â  Â  Â  Â  Â  Â  const finalOwnership = ownership * (1 - dilution);
Â  Â  Â  Â  Â  Â  const exitVal = arr * 1000000 * multiple;
Â  Â  Â  Â  Â  Â  const returnAmount = exitVal * finalOwnership;
Â  Â  Â  Â  Â  Â  const moic = returnAmount / invest;
Â  Â  Â  Â  Â  Â  const irr = (Math.pow(moic, 1/years) - 1) * 100;
Â  Â  Â  Â  Â  Â  const fundImpact = (returnAmount / fundSize) * 100;
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  document.getElementById('ownershipDisplay').textContent = (ownership * 100).toFixed(1) + '%';
Â  Â  Â  Â  Â  Â  document.getElementById('exitVal').textContent = '$' + (exitVal / 1000000).toFixed(0) + 'M';
Â  Â  Â  Â  Â  Â  document.getElementById('returnVal').textContent = '$' + (returnAmount / 1000).toFixed(0) + 'K';
Â  Â  Â  Â  Â  Â  document.getElementById('roiVal').textContent = moic.toFixed(1) + 'x';
Â  Â  Â  Â  Â  Â  document.getElementById('irrVal').textContent = irr.toFixed(0) + '%';
Â  Â  Â  Â  Â  Â  document.getElementById('fundVal').textContent = fundImpact.toFixed(1) + '%';
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  let verdict = 'âŒ';
Â  Â  Â  Â  Â  Â  if (moic >= 7) verdict = 'âœ…';
Â  Â  Â  Â  Â  Â  else if (moic >= 3) verdict = 'âš ï¸';
Â  Â  Â  Â  Â  Â  document.getElementById('verdict').textContent = verdict;
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  const currentARR = 1500 * 12; // $1.5k MRR * 12 = $18k ARR
Â  Â  Â  Â  Â  Â  const targetARR = arr * 1000000;
Â  Â  Â  Â  Â  Â  const growthRequired = (targetARR / currentARR).toFixed(0);
Â  Â  Â  Â  Â  Â  // Calculate months needed for growth at 20% MoM (a strong benchmark pace)
Â  Â  Â  Â  Â  Â  const monthsAt20 = Math.log(targetARR / currentARR) / Math.log(1.20);
Â  Â  Â  Â  Â  Â Â 
Â  Â  Â  Â  Â  Â  let notes = '';
Â  Â  Â  Â  Â  Â  if (moic < 3) {
Â  Â  Â  Â  Â  Â  Â  Â  notes = `Returns (${moic.toFixed(1)}x) are well below fund targets (need 5-10x). Requires ${growthRequired}x ARR growth. At 20% MoM needs ${monthsAt20.toFixed(0)} perfect months. Risk of capital loss is extremely high.`;
Â  Â  Â  Â  Â  Â  } else if (moic < 5) {
Â  Â  Â  Â  Â  Â  Â  Â  notes = `Base returns (${moic.toFixed(1)}x) are marginal for the risk profile. ${growthRequired}x growth is needed over ${monthsAt20.toFixed(0)} months at a perfect 20% MoM. A bridge round is likely, increasing dilution risk and further suppressing returns.`;
Â  Â  Â  Â  Â  Â  } else {
Â  Â  Â  Â  Â  Â  Â  Â  notes = `Returns meet minimums IF assumptions hold. Reaching $${arr}M ARR needs ${monthsAt20.toFixed(0)} months at 20% MoM (90th percentile pace). Must fix retention (March-April churn) and execute flawlessly on the AI voice agent.`;
Â  Â  Â  Â  Â  Â  }
Â  Â  Â  Â  Â  Â  document.getElementById('analysisNotes').textContent = notes;
Â  Â  Â  Â  }
